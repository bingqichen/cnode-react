{
  "name": "cnode-react",
  "version": "0.0.1",
  "description": "cnode社区react版",
  "main": "src/app.js",
  "scripts": {
    "server": "export NODE_SERVER=true && pm2 start app.js -n cnode-react --watch server",
    "dev": "NODE_ENV='development' webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "build": "NODE_ENV='production' webpack --progress --display-error-details --display-optimization-bailout --color --profile --config ./webpack/webpack.config.prod.js",
    "dll": "webpack --progress --display-error-details --display-optimization-bailout --color --profile --config ./webpack/webpack.config.dll.js",
    "test": "NODE_ENV='production' mocha --compilers js:babel-core/register --require ./test/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingqichen/cnode-react.git"
  },
  "keywords": [
    "cnode",
    "react",
    "redux",
    "ssr",
    "koa2"
  ],
  "author": "BingqiChen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bingqichen/cnode-react/issues"
  },
  "homepage": "https://github.com/bingqichen/cnode-react#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "dva": "^1.2.1",
    "dva-loading": "^0.2.1",
    "koa": "^2.3.0",
    "koa-pug": "^3.0.0-2",
    "koa-router": "^7.2.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-replace": "^0.6.1",
    "jsdom": "^11.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mocha": "^3.4.2",
    "mockjs": "^1.0.1-beta3",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0",
    "webstorm-disable-index": "^1.2.0"
  }
